
<!-- src/main/webapp/WEB-INF/web.xml -->
<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://jakarta.ee/xml/ns/jakartaee
                             http://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
         version="5.0">

  <welcome-file-list>
    <welcome-file>jsp/Landing.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
      <servlet-name>StaffLoginController</servlet-name>
      <servlet-class>controller.StaffLoginController</servlet-class>
  </servlet>

  <servlet-mapping>
      <servlet-name>StaffLoginController</servlet-name>
      <url-pattern>/StaffLoginController</url-pattern>
  </servlet-mapping>

</web-app>
